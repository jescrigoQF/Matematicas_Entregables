SET S /S-1*S-4/;
*SET TIME /T0,T1,T2/;
*SET ACTIVOS /X1, X2/;
*SET POSIBILIDADES /UU, UD, DU, DD/;

VARIABLES RP1,X11,X21,X121,X221,W(S),Y(S);
POSITIVE VARIABLES X11,X21,X121,X221,W(S),Y(S);

EQUATIONS FO,R0,R1,R3;

FO.. RP1=E=Y('S-1')-4*W('S-1');
R0.. X11+X21=E=55;
R1.. -1.25*X11-1.14*X21+X121+X221=E=0;
R3.. 1.25*X121+1.14*X221-Y('S-1')+ W('S-1')=E=70;




MODEL MARKOWIT /ALL/;

SOLVE MARKOWIT USING NLP MAXIMIZING RP1;